    "react-dom": "^18.2.0",
    "rescript": "*",
    "rescript-webapi": "^0.7.0"
  },
  "devDependencies": {
    "@jihchi/vite-plugin-rescript": "*",
    "@vitejs/plugin-react": "*",
    "prettier": "^2.8.1",
