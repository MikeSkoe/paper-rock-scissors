{
  "name": "app",
  "suffix": ".bs.js",
  "jsx": { "version": 4, "mode": "automatic" },
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header",
    "-open RescriptCore"
  ],
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "bs-dependencies": [
    "@mikeskoe/rex",
    "@rescript/react",
    "@rescript/core",
    "@ryyppy/rescript-promise",
    "bs-fetch",
    "rescript-webapi"
  ],
  "bs-dev-dependencies": []
}
